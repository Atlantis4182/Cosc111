package assignment5;

public class Q2 {
	public static void main(String[] args) {
	
		int sum = 1;
		
		for (int number = 1; number<10001; number++ ) {
			
			for (int x = 1; x < number; x++) {
				if (number%x == 0) { //dividable
					sum = sum + x;
					if (sum == number) {
						System.out.println(number);
					}
					
					
					
				}
			}
		}
		
	}
}