package assignment5;

public class Q2 {
	public static void main(String[] args) {
	
		int sum = 0;
		
		for (int number = 10000; number>0; number-- ) {
			
			for (int x = 1; x < 10001; x++) {
				if (number%x == 0) { //dividable
					sum = sum + x;
				}if (sum == number) {
						System.out.println(sum);
					}
					
					
					
				
			}
		}
		
	}
}