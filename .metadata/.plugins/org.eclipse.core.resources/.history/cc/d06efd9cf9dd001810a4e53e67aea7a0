package lec15;

public class Ex4 {
	public static void main(String[] args) {
		int[][] nums = {              // rows and columns start at 0, like the arrays they are
				
					{1,2,3}, // 0,1,2
					{4,5,6}, // 0,1,2
					{7,8,9}, // 0,1,2
					{7,8,9}
			};
		System.out.println(nums.length); // length of rows, # of rows
		System.out.println(nums[0].length);
	}
}
