package assignment5;

public class Q2 {
	public static void main(String[] args) {
	
		int sum = 0;

		for (int i = 1; i <= 10000; i++) {
		
			for(int a = 1; a < i; a++) {
				int divisor = a%i;
				if (divisor == 0) {
					sum = sum + a;
				}
				
			}
			if (sum == i)
				System.out.println(sum);
		}
					
		
	}
}