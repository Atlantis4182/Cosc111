package lec15;

public class Ex4 {
	public static void main(String[] args) {
		int[][] nums = {              // rows and columns start at 0, like the arrays they are
				
					{1,2,3,4,5}, // 0,1,2
					{1,2,3,4}, // 0,1,2
					{1,2,3}, // 0,1,2
					{1,2,},
					{1}
					
					
			};
		int[][] arr = new int [2] [3];
		
		System.out.println(nums.length); // length of rows, # of rows
		System.out.println(nums[4].length); //# columns. bc you enter the row's specific array asking how long in that row...which is the columns
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length; j++) {
				System.out.println(arr[i][j] + " ");
			}
		}
		}
}
