package lec14;

import java.util.Arrays;

public class Ex3 {
	public static void main(String[] args) {
		int z = 0;
		int[] x = {1,2,3,4,5};
		
		System.out.println(z);
		incrPrimitive(z);
		System.out.println(z);
	}
	

public static void incrPrimitive(int a) {
	a++;
	
}
	public static void increElements(int[] arr) {
		for (int i = 0;  i< arr.length; i++) {
			arr[i]++;			
		}
	}
}
