package lec15;

public class Ex4 {
	public static void main(String[] args) {
		int[][] nums = {              // rows and columns start at 0, like the arrays they are
				
					{1,2,3,4,5}, // 0,1,2
					{1,2,3,4}, // 0,1,2
					{1,2,3}, // 0,1,2
					{1,2,},
					{1}
					
					
			};
		System.out.println(nums.length); // length of rows, # of rows
		System.out.println(nums[4].length); //# columns
		
		int[][] arr = new int [3][];
		arr[0] = new int[3];
		arr[1] = new int[10];
		arr[2] = new int [5];
	}
}
