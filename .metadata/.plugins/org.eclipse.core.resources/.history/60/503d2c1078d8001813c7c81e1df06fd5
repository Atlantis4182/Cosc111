package lec13;

public class Ex3 {
	public static void main(String[] args) {
		
		
		int[] nums = new int[100];
		for (int i =0; i< nums.length ;i++)
			nums[i] = 5;
		
		int[] nums2 = new int [100];
		for (int i = 0; i< nums.length;i++) {
			nums2[i] = (int)(Math.random()*100);
			System.out.println(nums[i]);

		}
		
	}
}
