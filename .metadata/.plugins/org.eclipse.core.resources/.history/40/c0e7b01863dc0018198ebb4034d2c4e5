package lec14;

public class Ex1 {
	public static void main(String[] args) {
	
	int[] a = {1,2,3};
	int[] b = {5,6,7};
	
	
	a=b;
	System.out.println("A");
	for (int i = 0; i < a.length; i++) {
		System.out.println(a[i]);
	}
	System.out.println("B");
	for (int i = 0; i < b.length; i++) {
		System.out.println(b[i]);
	}
		
		
	}
}
