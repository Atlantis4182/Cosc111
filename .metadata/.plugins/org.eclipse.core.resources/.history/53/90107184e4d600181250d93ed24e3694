package lec12;

public class Ex2 {
	public static void main(String[] args) {
		
		System.out.println(max_int(3,5));
		System.out.println(max_double(3.1,5.2));

	}
	
	public static double max(double a, double b) {
		if(a>b) return a;
		else    return b;
	}
	public static int max(int a,int b) {
		if(a>b) return a;
		else    return b;
	}
}
