package assignment5;

public class Q2 {
	public static void main(String[] args) {
	

		for (int i = 1; i <= 10000; i++) {
			int sum = 0;
		
			for(int a = 1; a < i; a++) {
				if ((i%a) == 0) {
					sum = sum + a;
				}
				
			}
			if (sum == i) 
				System.out.println(sum);
		}
					
		int min = 1;
		int max = 10000;

		for (min = 1; min <= max; min++) {
		    int sum = 0;
		    int e = 1;
		    for (e = 1; e < min; e++) {
		        int a = min % e;

		        if (a == 0) {
		            sum += e;
		        }
		    } 
		    if (sum == min){           
		        System.out.println(sum);
		    }         
		}     
		
	}
}